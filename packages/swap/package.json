{
  "name": "@project-serum/swap",
  "version": "0.1.0-alpha.2",
  "description": "Client for swapping on the Serum DEX",
  "license": "MIT",
  "repository": "project-serum/serum-ts",
  "main": "lib/index.js",
  "source": "src/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc --watch",
		"clean": "rm -rf lib",
    "docs": "typedoc --excludePrivate --out ../../docs/swap src/index.ts --includeVersion --readme none",
    "prepare": "run-s clean build",
    "shell": "node -e \"$(< shell)\" -i --experimental-repl-await"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.21.9",
    "typescript": "^4.4.2"
  },
  "files": [
    "lib"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.14.0",
    "@project-serum/serum": "^0.13.58",
    "@solana/spl-token": "^0.1.8",
    "@solana/spl-token-registry": "^0.2.231",
    "@solana/web3.js": "^1.24.2",
    "base64-js": "^1.5.1",
    "bn.js": "^5.2.0"
