    "@project-serum/lockup": "^0.0.1-alpha.2",
    "@solana/spl-token": "0.0.11",
    "bn.js": "^5.1.2",
    "buffer-layout": "^1.2.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^0.86.1"
  }
}
